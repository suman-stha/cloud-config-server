eureka:
  instance:
    prefer-ip-address: true
  client:
    fetch-registry: true
    register-with-eureka: true
    service-url:
      defaultZone: http://localhost:8761/eureka


microservice:
  payment-service:
    endpoints:
      endpoint:
        uri: http://Payment-Service/payment/doPayment
  order-service:
    endpoints:
      endpoint: http://Order-Service/order/bookOrder
